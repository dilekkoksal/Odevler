ay=int(input("dogdugunuz ayi rakamsal olarak giriniz="))
gun=int(input("dogdugunuz gunu rakamsal olarak giriniz="))
if ay==1:
    if gun<=21:
        burc="oglak"
    elif gun>21:
        burc="kova"
elif ay==2:
    if gun <=19:
        burc="kova"
    elif gun>19:
        burc="balik"
elif ay==3:
    if gun <=21:
        burc="balik"
    elif gun>21:
        burc="koc"
elif ay==4:
    if gun<=21:
        burc="koc"
    elif gun>21:
        burc="boga"
elif ay ==5:
    if gun<=21:
        burc="boga"
    elif gun>21:
        burc="ikizler"
elif ay ==6:
    if gun<=22:
        burc="ikizler"
    elif gun>22:
        burc="yengec"
elif ay ==7:
    if gun<=22:
        burc="yengec"
    elif gun>22:
        burc="aslan"
elif ay ==8:
    if gun<=23:
        burc="aslan"
    elif gun>23:
        burc="basak"
elif ay ==9:
    if gun<=23:
        burc="basak"
    elif gun>23:
        burc="terazi"
elif ay ==10:
    if gun<=23:
        burc="terazi"
    elif gun>23:
        burc="akrep"
elif ay ==11:
    if gun<=22:
        burc="akrep"
    elif gun>22:
        burc="yay"
elif ay ==12:
    if gun<=21:
        burc="yay"
    elif gun>21:
        burc="oglak"


print("Burcunuz= "+burc)
